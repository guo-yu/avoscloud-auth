{
  "name": "avoscloud-auth",
  "version": "0.1.0",
  "description": "the auth service of avoscloud based on Angular.js and ionic UI",
  "main": "index.js",
  "scripts": {
    "serve": "node_modules/.bin/serve .",
    "watch": "node_modules/.bin/rewatch src/*.js -c 'npm run build'",
    "build": "node_modules/.bin/uglifyjs src/avoscloud-auth.js --mangle --compress -o dist/avoscloud-auth.min.js --source-map dist/avoscloud-auth.min.js.map --source-map-url avoscloud-auth.min.js.map --source-map-root ../src/avoscloud-auth.js",
    "dev": "npm run serve & npm run watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/turingou/avoscloud-auth.git"
  },
  "keywords": [
    "avoscloud",
    "angular"
  ],
  "author": "turing <o.u.turing@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/turingou/avoscloud-auth/issues"
  },
  "homepage": "https://github.com/turingou/avoscloud-auth",
  "devDependencies": {
    "serve": "~1.4.0",
    "uglify-js": "^2.4.15",
    "rewatch": "~0.2.2"
  }
}
